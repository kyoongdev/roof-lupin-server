{
  "name": "roof-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "local": "yarn config:local && nest start --watch",
    "stage": "yarn config:stage && nest start --watch",
    "dev": "yarn config:dev &&  nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:docker": "yarn prisma db push --accept-data-loss && node dist/main.js",
    "deploy:docker": "yarn prisma db push && node dist/main.js",
    "memory": "node dist/main --max-old-space-size=256",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier:fix": "prettier **/*.ts   --write",
    "lint:fix": "eslint --fix --ext .js,.ts ./src",
    "pre-commit": "yarn lint-staged",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "config:local": "cp ./env/.env.local .env && cp ./env/docker/Dockerfile.local Dockerfile",
    "config:dev": "cp ./env/.env.dev .env && cp ./env/docker/Dockerfile.dev Dockerfile",
    "config:docker": "cp ./env/.env.docker .env && cp ./env/docker/Dockerfile.local Dockerfile",
    "config:stage": "cp ./env/.env.stage .env ",
    "migrate:dev": "yarn config:dev && yarn prisma migrate dev",
    "migrate:local": "yarn config:local && yarn prisma migrate dev",
    "docker-compose:local": "yarn config:docker &&  docker-compose up --build -d",
    "seed": "ts-node src/seed.ts",
    "docs": "yarn prisma-docs-generator serve -p 8001",
    "docker:dev": "yarn config:dev && docker buildx build --platform linux/amd64,linux/arm64 -t roof . "
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.354.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.14.1",
    "axios": "^1.4.0",
    "cache-manager": "^5.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto": "^1.0.1",
    "exceljs": "^4.3.0",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.9.0",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "3.3.4",
    "nest-winston": "^1.9.2",
    "node-schedule": "^2.1.1",
    "prisma": "^4.14.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.2.0",
    "wemacu-nestjs": "^1.7.1",
    "winston": "^3.8.2"
  },
  "lint-staged": {
    "src/**/*.**.{js,ts}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ],
    "src/**/*.{js,ts}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@mermaid-js/mermaid-cli": "^10.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@prisma/generator-helper": "^4.14.1",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "18.15.11",
    "@types/node-schedule": "^2.1.0",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.3.2",
    "prettier-plugin-prisma": "^4.13.0",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-docs-generator": "^0.8.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "types": "./src/@types"
}
